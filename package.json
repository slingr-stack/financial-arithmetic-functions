{
	"name": "@slingr/financial-arithmetic-functions",
	"version": "3.3.0",
	"description": "A set of functions for doing math on numbers without floating point numbers",
	"main": "index.cjs",
	"module": "index.mjs",
	"exports": {
		".": {
			"require": "./index.cjs",
			"import": "./index.mjs",
			"types": "./index.d.ts"
		}
	},
	"scripts": {
		"build_test": "esbuild test/test.ts --bundle --format=esm --outfile=test/test.mjs",
		"test:node": "npm run build_test && node test/test.mjs",
		"test:readme": "jsmd readme.md",
		"test:types": "tsc --noEmit",
		"test": "run-p test:*",
		"build:types": "tsc --declaration --emitDeclarationOnly --outDir .",
		"build:cjs": "tsc --module commonjs --outDir . --target es2017",
		"build:esm": "tsc --module es2020 --outDir . --target es2017 && node -e \"require('fs').renameSync('index.js','index.mjs')\"",
		"prepare": "npm run build",
		"prepublishOnly": "npm run build",
		"build": "npm run build:types && npm run build:cjs && npm run build:esm"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TehShrike/financial-arithmetic-functions.git"
	},
	"keywords": [
		"financial",
		"arithmetic",
		"math",
		"money",
		"number",
		"bigint",
		"bignumber"
	],
	"author": "TehShrike",
	"license": "WTFPL",
	"bugs": {
		"url": "https://github.com/TehShrike/financial-arithmetic-functions/issues"
	},
	"homepage": "https://github.com/TehShrike/financial-arithmetic-functions#readme",
	"devDependencies": {
		"@types/node": "^22.13.4",
		"esbuild": "^0.14.39",
		"jsmd": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.7.3",
		"uvu": "^0.5.6"
	}
}